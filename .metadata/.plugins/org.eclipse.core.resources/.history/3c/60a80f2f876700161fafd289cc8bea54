package Bai1_7;

import static org.junit.Assert.*;

import java.io.IOException;

import org.junit.Test;

public class CDTest {

	@Test
	public void test1() throws IOException {
		CD cd1 = new CD("1", "Trai tim em cung biet dau", "Bao Anh", 2, 200000);
		ListOfCD list = new ListOfCD();
		list.enterCD();
		double ex = 200000;
		double ac = list.totalPrice();
		assertEquals(ex, ac,0.01);
	}

}
